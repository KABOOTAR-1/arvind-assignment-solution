{
  "name": "toteminteractive",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup-db": "node src/scripts/setupDatabase.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@huggingface/inference": "^4.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "node-cache": "^5.1.2",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "@babel/preset-env": "^7.23.0",
    "@babel/core": "^7.23.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testMatch": ["**/__tests__/**/*.js", "**/?(*.)+(spec|test).js"],
    "setupFilesAfterEnv": ["<rootDir>/src/__tests__/setup.js"],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/scripts/**",
      "!src/config/**",
      "!src/__tests__/**"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}
